{
  "name": "imuter",
  "version": "0.6.0",
  "description": "Immutable data library",
  "homepage": "https://github.com/jbedard/imuter",
  "keywords": [
    "immutable",
    "freeze",
    "frozen",
    "onpush",
    "observable"
  ],
  "main": "./dist/umd/imuter.js",
  "module": "./dist/esm/imuter.js",
  "esm5": "./dist/esm/imuter.js",
  "es2015": "./dist/es2015/imuter.js",
  "esm2015": "./dist/es2015/imuter.js",
  "fesm5": "./dist/esm/imuter.js",
  "fesm2015": "./dist/es2015/imuter.js",
  "types": "dist/imuter.d.ts",
  "side-effects": false,
  "author": "Jason Bedard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbedard/imuter.git"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/node": "12",
    "conventional-changelog-cli": "2.0",
    "dtslint": "^2.0.2",
    "istanbul": "0.4",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "3.1",
    "karma-coverage": "2.0",
    "karma-jasmine": "2.0",
    "karma-typescript": "4.1",
    "tslint": "^5.20.1",
    "typescript": "3.6",
    "uglify-es": "3.3"
  },
  "scripts": {
    "build": "yarn lint && yarn dtslint && yarn test && yarn compile && yarn uglify",
    "clean": "rm -Rf dist coverage",
    "compile": "yarn compile.esnext && yarn compile.es2015 && yarn compile.esm && yarn compile.amd && yarn compile.cjs && yarn compile.umd",
    "compile.esnext": "tsc --module esnext --target esnext --outDir dist/",
    "compile.es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "compile.esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "compile.amd": "tsc --module amd --target es5 --outDir dist/amd",
    "compile.cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "compile.umd": "tsc --module umd --target es5 --outDir dist/umd",
    "uglify": "uglifyjs -c -m --toplevel --define process.env.NODE_ENV='\"production\"' --define FREEZING_ENABLED=false --source-map 'content=dist/imuter.js.map' -o dist/imuter.min.js dist/umd/imuter.js",
    "test": "karma start karma.conf.js --single-run --browsers ChromeHeadless",
    "test-server": "karma start karma.conf.js",
    "dtslint": "dtslint type-specs",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json",
    "changelog": "conventional-changelog --infile ./CHANGELOG.md --same-file --release-count 1 --preset angular"
  },
  "peerDependencies": {
    "tslib": "^1.10.0"
  }
}
